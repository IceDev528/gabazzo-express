		$(document).ready(function(){
			$('.filter_toggler').click(function(){
				$('.the_filter').toggleClass('active');
				$('.filter_toggler').toggleClass('active');
			})
		})
		$(document).on("click", function(event){
			var $trigger = $(".filter_toggler_wrap");
			if($trigger !== event.target && !$trigger.has(event.target).length){
				$(".the_filter").removeClass("active");
				$(".filter_toggler").removeClass("active");
			}            
		});
		</script>
			
