		// Initialize Splide For Thumnail Slider
		document.addEventListener( 'DOMContentLoaded', function () {
			var main = new Splide( '.main_slider .splide', {
				type      : 'fade',
				rewind    : false,
				pagination: false,
				arrows    : false,
			});

			var thumbnails = new Splide( '.slider_thumbnails .splide', {
				perPage     : 6,
				perMove     : 1,
				gap         : 5,
				rewind      : false,
				pagination  : false,
				arrows      : true,
				autoplay    : false,
				cover       : false,
				isNavigation: true,
				breakpoints : {
					580: {
						perPage : 3,
					},
					767: {
						perPage : 4,
					},
					993: {
						perPage : 5,
					},
				},
			});

			main.sync( thumbnails );
			main.mount();
			thumbnails.mount();
		});
		</script>

