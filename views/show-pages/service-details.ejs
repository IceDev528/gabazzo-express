		<!--===== Markup For "Service Port" Starts From Here =====-->
		<section class="service_port">
			<div class="container">
				<div class="company_info_facts">
					<div class="company_logo">
						<a href="../company-profile/<%= service.owner.id._id %>"><img src="<%= service.owner.id.logo %>" class="img-fluid" alt="" /></a>
					</div>
					<div class="company_infos grid_item">
						<h4>
							<a  style="color: inherit; text-decoration: inherit;"  href="../company-profile/<%= service.owner.id._id %>"><span class="company_name"><%= service.owner.id.companyName %></span></a>
							<% let intValue = parseInt(service.owner.id.averageReview), flagValue = false; leftOver = 0 %>
								<% for(let i=0; i< intValue; i++){ %>
										<i class="fas fa-star"></i>
								<% }%>
								<% leftOver = 5 - intValue %>
								<% if(!Number.isInteger(service.owner.id.averageReview)){ %>
								<% let decimalPart = service.owner.id.averageReview % 1; %>
								<% if(decimalPart == 0.5){ %>
									<i class="fas fa-star-half-alt"></i>
									<% flagValue = true %>
								<% } else if(Math.round(decimalPart) > 0.5) { %>
									<i class="fas fa-star"></i>
									<% flagValue = true %>
								<% } %>
								<% }%>
									<% if(flagValue) {%>
										<% leftOver = leftOver - 1 %>
									<% } %>
									<% for(let i=0; i< leftOver; i++){ %>
										<i class="far fa-star"></i>
									<% }%>
							<span class="mean_ratings"><%= service.owner.id.averageReview %></span>
							<span class="review_amount">(<%= review.length %>)</span>
							<!-- <span class="orders_in_queue">2 Orders in Queue</span> -->
						</h4>
					</div>
				</div>

				<div class="service_title_facts">
					<div class="service_title grid_item">
						<h3><%= service.title %></h3>
					</div>

					<div class="service_facts">
						<ul>
							<li><span class="gbtn gbtn_dark"><i class="far fa-clock"></i> <%= service.time %> Days</span></li>
							<li><span class="gbtn gbtn_dark">Estimate: $<%= service.priceFrom %> - $<%= service.priceTo %></span></li>
						</ul>
					</div>
				</div>

				<div class="port_slider_quote">
					<div class="service_port_all_info">
						<div class="port_slider">
							<ul id="port-gallery" class="gallery list-unstyled">
								<% service.images.map((image) => { %>
								<li data-thumb="<%= image.url %>"> 
									<img src="<%= image.url %>" />
								</li>
								<%}) %>
							</ul>
						</div>

						<div class="service_description" id="serviceDescription">
							<h3>About this Service Port</h3>
							<p><%= service.description %></p>
						</div>

						<div class="received_quote_box mobile_only">
							<div class="title_head">
								<h4>Receive Quote</h4>
							</div>
							<div class="quote_details">
								<div class="pricing">
									<h2>$<%= service.priceFrom %> - $<%= service.priceTo %></h2>
								</div>
								<div class="features_points">
									<ul>
										<li>Transparent Transaction</li>
										<li> Gabazzo Support Team Dedicated</li>
									</ul>
								</div>
								<div class="action_buttons">
									<ul>
										<% if(currentUser){ %>
											<li><a class="gbtn gbtn_bdr" href="javascript:void(0)" data-target="#theMessagePopup" data-toggle="modal">Contact Us</a></li>
										<% } else { %>
											<li><a class="gbtn gbtn_bdr"   href="javascript:void(0)" data-target="#loginForm" data-toggle="modal" >Contact Us</a></li>
										<% } %>
									</ul>
								</div>
							</div>
						</div>

						<% if(service.filter) { %>
						<div class="tags_category">
							<% let counter = 0 %>
							<% let colors = ['gbadge_orange', 'gbadge_red', 'gbadge_purple'] %>
							<% for(filter in service.filter){ %>
								<div class="tags_single_category">
								<div class="service_type_heading">
									<h5><%= filter %>:</h5>
								</div>
								<div class="available_tags">
									<ul>
									<% service.filter[filter].map((item, i) => { %>
										<li><span class="gbadge <%= colors[counter] %>" ><%= item %></span></li>
									<% }) %>
									</ul>
								</div>
