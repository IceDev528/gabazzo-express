											<% if(currentUser && !currentUser.isCompany) { %>
												<li class="dropdown">
													<a class="gbtn gbtn_bdr" href="javascript:void(0)"
														data-target="#savedListDown" data-toggle="dropdown">Save to
														List</a>
													<div class="dropdown-menu saved_list_down"
