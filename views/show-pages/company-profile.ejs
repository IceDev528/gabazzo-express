												<% } %>
													
												<!-- <li class="seller_badges_link"><a class="gbtn gbtn_dark" href="javascript:void(0)" data-target="#sellerBadges" data-toggle="modal"><img src="../images/seller_badges_icon.png" alt=""> Seller Badges</a></li> -->
													
																				
												</ul>
											</div>
										</div>
									</div>
								</div>
