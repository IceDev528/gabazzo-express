										<% } %>
										<div class="like_unlike">
											<ul>
												<li><a href="#reportAgainstReview" data-toggle="modal">Report</a></li>
											</ul>
											<a class="btn btn-outline-gbz-success reply_reviews_button"
												href="javascript:void(0)">Comment</a>
											<div class="reply_fields mt-3">
												<form action="/review/<%= review[i]._id %>?_method=PUT" method="POST">
													<div class="form">
														<div class="form-group">
															<textarea name="reply" id="" cols="30" rows="2"
																class="form-control"></textarea>
														</div>
														<div class="action_buttons">
															<ul>
																<li><a class="btn reply_reviews_cancel_button btn-outline-gbz-success"
																		href="javascript:void(0)">Cancel</a></li>
																<li><button class="btn btn-gbz-success"
																		href="javascript:void(0)"
																		type="submit">Comment</button></li>
															</ul>
														</div>
													</div>
												</form>
											</div>
										</div>
									</div>

									<div class="col-md-3">
										<div class="review_related_image_slider">
											<div id="reviewsSlider1" class="carousel slider_part_inner slide"
												data-ride="carousel">
												<!-- Indicators -->
												<ul class="carousel-indicators">
													<li data-target="#reviewsSlider1" data-slide-to="0" class="active">
													</li>
													<li data-target="#reviewsSlider1" data-slide-to="1"></li>
													<li data-target="#reviewsSlider1" data-slide-to="2"></li>
												</ul>

												<!-- The slideshow -->
												<div class="carousel-inner">
													<div class="carousel-item text-center active">
														<a class="lightcase" href="<%= review[i].images[0].url %>"
															data-rel="reviewsSlider1_img:carousel">
															<img src="<%= review[i].images[0].url %>" class="img-fluid"
																alt="" />
														</a>
													</div>
													<% review[i].images.forEach(function(image) { %>
													<div class="carousel-item text-center">
														<a class="lightcase" href="<%= image.url %>"
															data-rel="reviewsSlider1_img:carousel">
															<img src="<%= image.url %>" class="img-fluid" alt="" />
														</a>
													</div>
													<% }); %>
												</div>

												<!-- Left and right controls -->
												<a class="carousel-control-prev" href="#reviewsSlider1"
													data-slide="prev">
													<span class="carousel-control-prev-icon"></span>
												</a>
												<a class="carousel-control-next" href="#reviewsSlider1"
													data-slide="next">
													<span class="carousel-control-next-icon"></span>
												</a>
											</div>
										</div>
									</div>
								</div>
							</div>
							<% }; %>
						</div>
						<% } %>

					</div>
				</div>
			</div>
		</div>
	</section>


	<!-- Report Review Modal Starts From Here -->
	<div class="modal report_review_modal fade" id="reportAgainstReview">
		<div class="modal-dialog modal-lg modal-dialog-centered">
			<div class="modal-content">
				<div class="modal-header">
					<h3>Report Against This Review</h3>
					<button type="button" class="close" data-dismiss="modal"><i class="fa fa-times"
							aria-hidden="true"></i></button>
				</div>
				<div class="modal-body">
