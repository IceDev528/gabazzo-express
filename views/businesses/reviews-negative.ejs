	<section class="dashboard_middle">
		<div class="container_gbz">
			<div class="dashboard_reviews">
				<div class="dash_review_head">
					<h4>Reviews</h4>
				</div>

				<hr />

				<!-- Review Section Starts From Here -->
				<div class="cp_split_reviews">
					<div class="header">
						<div class="row">
							<div class="col-sm-6">
								<h3>Reviews <span class="star"><i class="fa fa-star" aria-hidden="true"></i>
										<%= average %></span>
									<span class="amount">(<%= review.length %>)</span></h3>
							</div>
							<div class="col-sm-2"></div>
							<div class="col-sm-4">
								<form action="/company-dashboard/reviews" method="POST" class="class=" filter-form">
									<select name="filter" id="" class="form-control" onchange="this.form.submit()">
										<option value="recent">Most Recent</option>
										<option value="positive">Positive Reviews</option>
										<option value="neutral">Neutral Reviews</option>
										<option value="negative" selected>Negative Reviews</option>
									</select>
								</form>
							</div>
						</div>
					</div>
					<div class="reviews_wrapper" id="list">
						<% if(review.length > 0) { %>
						<% for (let i = 0; i < review.length; i++) {%>
						<% if (i > 4) {break;} %>
						<div class="single_review">
							<div class="row">
								<div class="col-md-9">
									<h6><%= review[i].owner.username %> <span class="star"><i class="fa fa-star"
												aria-hidden="true"></i> <%= review[i].rating %>.0</span></h6>
									<p><%= review[i].text %></p>
									<span class="review_time"><%= moment(review[i].createdAt).fromNow() %></span>
									<% if (review[i].reply) { %>
									<p><strong>Reply: </strong><%= review[i].reply %></p>
									<% } %>
									<div class="like_unlike">
										<ul>
											<li><a href="#reportAgainstReview" data-toggle="modal">Report</a></li>
										</ul>
										<a class="btn btn-outline-gbz-success reply_reviews_button"
											href="javascript:void(0)">Comment</a>
										<div class="reply_fields mt-3">
											<form action="/review/<%= review[i]._id %>?_method=PUT" method="POST">
												<div class="form">
													<div class="form-group">
														<textarea name="reply" id="" cols="30" rows="2"
															class="form-control"></textarea>
													</div>
													<div class="action_buttons">
														<ul>
															<li><a class="btn reply_reviews_cancel_button btn-outline-gbz-success"
																	href="javascript:void(0)">Cancel</a></li>
															<li><button class="btn btn-gbz-success"
																	href="javascript:void(0)"
																	type="submit">Comment</button></li>
														</ul>
													</div>
												</div>
											</form>
										</div>
									</div>
								</div>

								<div class="col-md-3">
									<div class="review_related_image_slider">
										<div id="reviewsSlider1" class="carousel slider_part_inner slide"
											data-ride="carousel">
											<!-- Indicators -->
											<ul class="carousel-indicators">
												<li data-target="#reviewsSlider1" data-slide-to="0" class="active"></li>
												<li data-target="#reviewsSlider1" data-slide-to="1"></li>
												<li data-target="#reviewsSlider1" data-slide-to="2"></li>
											</ul>

											<!-- The slideshow -->
											<div class="carousel-inner">
												<div class="carousel-item text-center active">
													<a class="lightcase" href="<%= review[i].images[0].url %>"
														data-rel="reviewsSlider1_img:carousel">
														<img src="<%= review[i].images[0].url %>" class="img-fluid"
															alt="" />
													</a>
												</div>
												<% review[i].images.forEach(function(image) { %>
												<div class="carousel-item text-center">
													<a class="lightcase" href="<%= image.url %>"
														data-rel="reviewsSlider1_img:carousel">
														<img src="<%= image.url %>" class="img-fluid" alt="" />
													</a>
												</div>
												<% }); %>
											</div>

											<!-- Left and right controls -->
											<a class="carousel-control-prev" href="#reviewsSlider1" data-slide="prev">
												<span class="carousel-control-prev-icon"></span>
											</a>
											<a class="carousel-control-next" href="#reviewsSlider1" data-slide="next">
												<span class="carousel-control-next-icon"></span>
											</a>
										</div>
									</div>
								</div>
							</div>
						</div>
						<% }; %>
						<% } %>


						<div class="see_more pb-3">
							<a href="javascript:void(0)" onclick="myFunction()"><i class="fa fa-plus"
									aria-hidden="true"></i> Show More</a>
						</div>

						<% if(review.length > 5){ %>
						<div id="hiddenDiv" style="display:none;">
							<% for (let i = 5; i < review.length; i++) {%>
							<div class="single_review">
								<div class="row">
