	<script type="text/javascript">

		$('.reply_reviews_button').click(function () {
			//$('.hideme').hide();  
			if ($(this).hasClass('active')) {
				$(this).removeClass('active');
				$('.reply_fields').slideUp();
			} else {
				$('.reply_fields').slideUp();
				$('.reply_reviews_button').removeClass('active');
				$(this).addClass('active');
				$(this).next().filter('.reply_fields').slideDown();
			}
		});


		$(document).ready(function () {
			$('.reply_reviews_cancel_button').click(function () {
				$('.reply_fields').slideUp();
				$('.reply_reviews_button').removeClass('active');
			})
		})

		$('.carousel').carousel({
			interval: false,
		})

		// For Slider Popups
		$(document).ready(function () {
			var lightcase = $(".lightcase");

			if (lightcase.length) {
				$('a[data-rel^=reviewsSlider1_img]').lightcase();
			}
			if (lightcase.length) {
				$('a[data-rel^=reviewsSlider2_img]').lightcase();
			}
			if (lightcase.length) {
				$('a[data-rel^=reviewsSlider3_img]').lightcase();
			}
			if (lightcase.length) {
				$('a[data-rel^=reviewsSlider4_img]').lightcase();
			}
		})


		$(function () {
			$('input[name=report_type]').on('click init-post-format', function () {
				$('#fakeReportBottom').toggle($('#fakeReport').prop('checked'));
			}).trigger('init-post-format');
		});

		$(function () {
			$('input[name=report_type]').on('click init-post-format', function () {
				$('#otherReportBottom').toggle($('#otherReport').prop('checked'));
			}).trigger('init-post-format');
		});

	</script>

	<script>
		function myFunction() {
			let x = document.getElementById("hiddenDiv");
			if (x.style.display === "none") {
				x.style.display = "block";
			} else {
				x.style.display = "none";
			}
		}
	</script>

