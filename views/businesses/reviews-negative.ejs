							<input type="radio" id="fakeReport" name="report_type">
							<span class="checkround"></span>
						</label>
						<label class="radio">Other
							<input type="radio" id="otherReport" name="report_type">
							<span class="checkround"></span>
						</label>
					</div>

					<div class="hidden_report_fields">
						<div id="fakeReportBottom">
