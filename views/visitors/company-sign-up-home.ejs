    <!--===== Markup For "Company Signup Header" Starts From Here =====-->
    <header class="cp_signup_header_wrap">
      <div class="overlay">
        <div class="top">
          <div class="container">
            <div class="top_contents">
              <h3>Work Your Way</h3>
              <p>Bring in your Company Skills and Receive Leads!</p>
              <div class="become_seller_link">
                <a href="/become-a-seller-overview" class="gbtn"
                  >Become a Seller</a
                >
              </div>
            </div>
          </div>
        </div>

        <div class="bottom">
          <div class="container">
            <div class="bottom_contents">
              <div class="highlighted_facts">
                <div class="single_fact">
                  <p>Registered Companies</p>
                  <h4><%= total.length %>+</h4>
                </div>
                <div class="single_fact">
                  <p>Commission</p>
                  <h4>8%</h4>
                </div>
                <div class="single_fact">
                  <p>Selling Price Range</p>
                  <h4>$25 - $999,999.99</h4>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </header>
    <!--===== Markup For "Company Signup Header" Ends Here =====-->

    <!--===== Markup For "Recently Joined" Starts From Here =====-->
    <section class="recently_joined_companies">
      <div class="container">
        <div class="recently_joined owl_controls_outer">
          <div class="cp_signup_heading">
            <h3>Companies Recently Joined</h3>
          </div>

          <div class="owl-carousel">
            <% companies.forEach(function(company, i) { %>
            <div class="single_company" style="min-height: 450px">
              <div class="logo_info">
                <div class="logo">
                  <a href="/company-profile/<%= company._id %>"
                    ><img
                      src="<%= company.logo %>"
                      class="img-fluid"
                      alt="Company Logo"
                  /></a>
                  <% if(company.companyStatus) {%>
                  <span class="activity_status online_status"></span>
                  <%} else {%>
                  <span class="activity_status offline_status"></span>
                  <% }%>
                </div>
                <div class="info" style="height: 80px">
                  <h5 style="text-transform: capitalize">
                    <%= company.companyName %>
                  </h5>
                  <ul>
                    <li><b><%= company.location %></b></li>
                    <li>
                      <% if(company.isHoliday) { %> <b> Closed </b> <% } else { %> <b> <%=
                      company.startTime + " - " + company.endTime %> </b> <% } %>
                    </li>
                  </ul>
                </div>
              </div>
              <div class="about">
                <h6>About</h6>
                <div class="para_texts">
                  <p class="more" style="min-height: 100px">
                    <%= company.about %>
                  </p>
                </div>
              </div>
              <div class="action_buttons">
                <ul>
                  <% if(!currentUser) { %>
                  <li>
                    <a
                      class="gbtn gbtn_dark"
                      href="javascript:void(0)"
                      data-target="#loginForm"
                      data-toggle="modal"
                      >Message</a
                    >
                  </li>
                  <% } else {%>
                  <li>
                    <a
                      class="gbtn gbtn_dark"
                      href="javascript:void(0)"
                      data-target="#messageModal<%= i %>"
                      data-toggle="modal"
                      >Message</a
                    >
                  </li>
                  <% }%>
                  <li>
                    <a
                      class="gbtn gbtn_dark_bdr"
                      href="/company-profile/<%= company._id %>"
                      >View More</a
                    >
                  </li>
                    >
                  </li>
                </ul>
              </div>
            </div>
            <% })%>
          </div>
        </div>
      </div>
    </section>
    <!--===== Markup For "Recently Joined" Ends Here =====-->

    <!--===== Markup For "How It Works" Starts From Here =====-->
    <section class="cp_signup_how_works">
      <div class="container">
        <div class="cp_signup_heading">
          <h3>How it Works</h3>
        </div>

        <div class="how_gb_works">
          <div class="single_steps">
            <div class="icon">
              <img
                src="../images/layout/bs_creating_icon.png"
                class="img-fluid"
                alt=""
              />
            </div>
            <div class="texts">
              <h4>1. Sign Up</h4>
              <p>
               Customize your company profile, create service ports
	       and start selling on Gabazzo
              </p>
            </div>
          </div>

          <div class="single_steps">
            <div class="icon">
              <img
                src="../images/layout/bs_delivery_icon.png"
                class="img-fluid"
                alt=""
              />
            </div>
            <div class="texts">
              <h4>2. Deliver Great Work</h4>
              <p>
                Get notified when you receive a potential client and use our
                system to discuss details with customers.
              </p>
            </div>
          </div>

          <div class="single_steps">
            <div class="icon">
              <img
                src="../images/layout/bs_completed_icon.png"
                class="img-fluid"
                alt=""
              />
            </div>
            <div class="texts">
              <h4>3. Receive Feedback</h4>
              <p>
                Once you've delivered your Service work on time, you'll
                receive a rating from the client.
              </p>
            </div>
          </div>
        </div>
      </div>
    </section>
    <!--===== Markup For "How It Works" Ends Here =====-->

    <!--===== Markup For "Right Tools" Starts From Here =====-->
    <section class="cp_signup_right_tools">
      <div class="container">
        <div class="business_tools">
          <div class="left_image">
            <div class="image">
              <img
                src="../images/layout/the_right_tool.png"
                class="img-fluid"
                alt=""
              />
            </div>
          </div>

          <div class="right_texts">
            <div class="cp_signup_heading">
              <h3>Give Your Business The Right Tools</h3>
            </div>

            <div class="all_info">
              <div class="single_info">
                <h4>Why do business with Gabazzo??</h4>
                <p>
                  Create your free company profile, display your service ports.
                  Receive free leads and make safe & secure transparent
                  transactions.
                </p>
              </div>
              <div class="single_info">
                <h4>Benefits for the Future</h4>
                <p>
                  Receive business tips, software, and other tools to run your
                  business. When BETA ends we’re releasing software to manage
                  your company in one place.
                </p>
              </div>
              <div class="single_info">
                <h4> Secured Payment</h4>
                <p>
