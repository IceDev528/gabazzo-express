    <!--Custom Script JS File-->
    <script type="text/javascript" src="js/updatedCustom.js"></script>
    <script src="../js/autocomplete.js" type="text/javascript"></script>
    <!--Custom JavaScript-->
    <script type="text/javascript">
      // Tracking System Navbar Links
      $(document).ready(function () {
        $(".dismiss_mobile_ts_menu").on("click", function () {
          $(".nav_with_bar .menu ul").removeClass("show");
        });

        $(".mobile_ts_menu_switch").on("click", function () {
          $(".nav_with_bar .menu ul").addClass("show");
        });

        $("#receivedWorkRequirements").on("shown.bs.modal", function (e) {
          // do something...
          $(document).ready(function () {
            $("#details").on("keyup", function (event) {
              maxLength = $("#details").val().length;
              $("#detailsMaxCounter").html(maxLength + "/2500");
            });
          });
        });

        $("button[name=startOrder]").on("click ", function () {
          var details = document.getElementById("details").value;
          var receivedDetails, serviceMentioned, clientReady;

          var eleReceivedDetails =
            document.getElementsByName("receivedDetails");
          for (i = 0; i < eleReceivedDetails.length; i++) {
            if (eleReceivedDetails[i].checked)
              receivedDetails = eleReceivedDetails[i].value;
          }

          var eleServiceMentioned =
            document.getElementsByName("serviceMentioned");
          for (i = 0; i < eleServiceMentioned.length; i++) {
            if (eleServiceMentioned[i].checked)
              serviceMentioned = eleServiceMentioned[i].value;
          }

          var eleClientReady = document.getElementsByName("clientReady");
          for (i = 0; i < eleClientReady.length; i++) {
            if (eleClientReady[i].checked)
              clientReady = eleClientReady[i].value;
          }

          if (
            receivedDetails === 'yes' &&
            serviceMentioned === 'yes' &&
            clientReady === 'yes' &&
            details !== ""
          ) {
            // alert("yes");
            $.ajax({
              method: "POST",
              url: "/company/add-desc",
              data: {
                job_id: jobId,
                description: details,
                answer1: receivedDetails,
                answer2: serviceMentioned,
                answer3: clientReady,
              },
            }).done(function (data) {
              console.log("data", data);
              // location.replace(`/manage-orders`);
              location.reload();
            });
          }
        });
      });
    </script>
