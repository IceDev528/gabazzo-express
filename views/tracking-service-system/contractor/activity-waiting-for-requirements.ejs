    <div class="modal fade requirements_modal" id="receivedWorkRequirements">
      <div class="modal-dialog modal-lg modal-dialog-centered">
        <div class="modal-content">
          <div class="modal-header">
            <h3>I Have Received These Following Work Requirements</h3>
            <button type="button" class="close" data-dismiss="modal">
              &times;
            </button>
          </div>
          <div class="modal-body">
            <div class="needed_requirements">
              <div class="single_requirements yes_no_header">
                <div class="requirements_title"></div>
                <div class="requirements_availability">
                  <ul>
                    <li>Yes</li>
                    <li>No</li>
                  </ul>
                </div>
              </div>

              <div class="single_requirements">
                <div class="requirements_title">
                  <h5>Have you received all details regarding this service?</h5>
                </div>
                <div class="requirements_availability">
                  <ul data-toggle="buttons">
                    <li class="btn">
                      <input
                        type="radio"
                        name="receivedDetails"
                        id="receivedDetails"
                        autocomplete="off"
                        value="yes"
                      />
                      <div class="circle circle_check"></div>
                    </li>
                    <li class="btn">
                      <input
                        type="radio"
                        name="receivedDetails"
                        id="receivedDetails"
                        autocomplete="off"
                        value="no"
                      />
                      <div class="circle circle_times"></div>
                    </li>
                  </ul>
                </div>
              </div>

              <div class="single_requirements">
