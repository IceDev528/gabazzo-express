          var companyAddress = document.getElementById("companyAddress").value;
          var receiptNumber = document.getElementById("receiptNumber").value;
          var rebateNumber = document.getElementById("rebateNumber").value;
          var date = document.getElementById("date").value;

          var stateTax = document.getElementById("stateTax").value;
          var discount = document.getElementById("discount").value;

          const descriptionArray = [];
          for (var i = 1; i < counter; i++) {
            var description = document.getElementById(`description${i}`).value;
            var qty = document.getElementById(`qty${i}`).value;
            var cost = document.getElementById(`cost${i}`).value;
            descriptionArray.push({
              description,
              quantity: qty,
              cost,
            });
          }

