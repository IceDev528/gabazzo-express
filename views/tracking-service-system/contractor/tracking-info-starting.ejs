            descriptionArray.length > 0 &&
            descriptionArray[0].description !== "" &&
            descriptionArray[0].quantity !== "" &&
            descriptionArray[0].cost !== "" &&
            companyName !== "" &&
            companyAddress !== "" &&
            receiptNumber !== "" &&
            rebateNumber !== "" &&
            date !== "" &&
            stateTax !== "" &&
            discount !== ""
          ) {
