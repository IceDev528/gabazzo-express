	<div class="modal fade msg_popup_modal" id="theMessagePopup">
		<div class="modal-dialog modal-lg modal-dialog-centered">
			<div class="modal-content">
				<div class="modal-header">
					<h3>Send a Message</h3>
					<button type="button" class="close" data-dismiss="modal">&times;</button>
				</div>
				<div class="modal-body">
					<div class="msg_pop_box">
						<div class="company_info">
							<div class="company_logo_name">
								<div class="company_logo">
									<a href="javascript:void(0)"><img src="<%= user.profilePicture %>" class="img-fluid" alt=""
										/></a>
										<span class="<%= user.online ? 'activity_status online_status' : '' %>" </span>
								</div>
								<div class="company_name">
									<h5>
										<%= user.firstName + " " + user.lastName %>
									</h5>
									<!-- <h6>
										<%= user.des %>
									</h6> -->
								</div>
							</div>

							<div class="need_requirements">
								<h5>Please Include:</h5>

								<ul>
									<li>Project Description</li>
									<li>Specific Instructions</li>
									<li>Relevant Files</li>
									<li>Your Budget</li>
									<li>Delivery Time</li>
								</ul>
							</div>
						</div>

						<div class="msg_box_wrap">
							<div class="time_zone">
								<h5>Time Zone: <span> 
									<%= time %>
									</span></h5>
							</div>

							<form action="/send" method="POST">
								<div class="msg_box">
									<textarea name="msg" id="field" onkeyup="countChar(this)" placeholder="Type your message..." cols="30" maxlength="2500"
										rows="10" class="form-control"></textarea>

									<div class="indicator_attach">
										<div class="character_indicator grid_item">
											<h6><span id="charNum">0</span>/2,500</h6>
										</div>
										<div class="attach_button">
											<div class="attach_file">
												<label class="attachment">
													<i class="fa fa-paperclip"></i>
													<input class="file_attachment" id="" name="attachFile" type="file">
												</label>
											</div>
										</div>
									</div>
								</div>

								<div class="submit_button">
									<button class="btn gbtn" type="submit">Send</button>
								</div>
							</form>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
	<!--===== Markup For "Message Popup" Ends Here =====-->
	
